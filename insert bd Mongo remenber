db.wands.insert({
  "name": "Dream Bender",
  "creator": "Foxmond",
  "level_required":10,
  "price":34.9,
  "powers":["Fire","Love"],
  "damage":{magic:4,melee:2}
  
})


Agregatiosn 

db.wands.aggregate([
  {"$group": {"_id": "$maker", 
              "total_wands": {"$sum": 1}, 
              "max_magic": {"$max": "$damage.magic"},
             "lowest_price": {"$min": "$price"}
             }}
])
